package org.javacoders;

import org.javacoders.cars.Corolla;
import org.javacoders.cars.Swift;
import org.javacoders.specs.EngineType;
import org.javacoders.specs.V6;
import org.javacoders.specs.V8;
import org.springframework.context.annotation.*;

@Configuration
@ComponentScan("org.javacoders")
public class AppConfig {
	
	@Bean("myCorolla")
	public Corolla corolla() {
		return new Corolla();
	}
	
	@Bean("swift")
	public Swift swift() {
		return new Swift();
	}
	
	@Bean("engineType")
	public EngineType engineType() {
		return new EngineType();
	}
}
